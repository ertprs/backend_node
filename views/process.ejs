<%- include('header', {title: title}); %>
<div class="content">
    
    <!-- Start Content-->
    <div class="container-fluid">
        <div class="row page-title">
            <div class="col-md-12">
                <nav aria-label="breadcrumb" class="float-right mt-1">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/">Home</a></li>
                        <li class="breadcrumb-item"><a href="/process">Process Order</a></li>
                    </ol>
                </nav>
                <h4 class="mb-1 mt-0">Process Order</h4>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped mb-0">
                                <thead>
                                    <tr>
                                    <th scope="col">Contact</th>
                                    <th scope="col">Message Order</th>
                                    <th scope="col">Quantity</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Courier</th>
                                    <th scope="col">Phone</th>
                                    <th scope="col">Address</th>
                                    <th scope="col">Action</th>
                                    </tr>
                                </thead>
                                <tbody id="tbody_inbox">                                    
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <td colspan="4">
                                            <div class="d-flex justify-content-center">
                                                <div id="div_pagination"></div>
                                            </div>
                                        </td>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- end row -->
        
        <div id="modal-print" class="modal" tabindex="-1" role="dialog">
          <div class="modal-dialog mw-100 w-75" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title">Choose Sender</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div class="modal-body">
                <form method="post" id="form-print" action="/print_order" target="_blank" class="needs-validation" novalidate>
                  <div class="form-row">
                    <div class="col-md-5 mb-3">
                      <label for="validationTooltip01">Store / Branch</label>
                      <input type="text" id="input-store" class="form-control" id="validationTooltip01" placeholder="" value="LANGGENG HERBAL" required>
                    </div>
                    <div class="col-md-4 mb-3">
                      <label for="validationTooltip01">Name</label>
                      <input id="input-name" type="text" class="form-control" id="validationTooltip01" placeholder="" value="AGUS SUGIHARTO" required>
                    </div>
                    <div class="col-md-3 mb-3">
                      <label for="validationTooltip01">Phone</label>
                      <input id="input-phone" type="text" class="form-control" id="validationTooltip01" placeholder="" value="021-2345754" required>
                    </div>
                  </div>
                  <div class="form-row">
                    <div class="col-md-12 mb-3">
                      <label for="validationTooltip01">Address</label>
                      <textarea id="input-address" class="form-control" id="validationTooltip01">JALAN BANGKA 2 #37, PELA MAMPANG, JAKARTA SELATAN, RT.9/RW.3, PELA MAMPANG, KEC. MAMPANG PRPT., KOTA JAKARTA SELATAN, DAERAH KHUSUS IBUKOTA JAKARTA 12170
                      </textarea>
                    </div>
                  </div>
                  <input type="hidden" id="data-id" />
                  <button class="btn btn-sm btn-primary float-right" type="submit"><i class="fa fa-cog"></i> Print Preview</button>
                  <button type="button" class="btn btn-sm btn-secondary float-left" data-dismiss="modal">Close</button>
                </form>
              </div>
            </div>
          </div>
        </div>

    </div> <!-- container-fluid -->

</div> <!-- content -->
<%- include('footer', {footer_text: footer_text}); %>